{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "../../../dashboard-v2/src/core/wallet/useLoginJwt.ts",
    "../../../dashboard-v2/src/core/wallet/providers/login.tsx",
    "../../../dashboard-v2/src/core/wallet/constants/login.ts",
    "../../../dashboard-v2/src/core/wallet/components/LoginButton.tsx"
  ]
}
